{
  "manifest_version": 3,
  "name": "SaveMyExams Revision Notes Hack",
  "version": "1.0",
  "description": "Selects specific elements on web pages",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}